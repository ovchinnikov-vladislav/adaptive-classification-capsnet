<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<GridPane xmlns="http://javafx.com/javafx"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="bmstu.dynamic.simulator.controller.SimulationController"
          visible="false">
    <rowConstraints>
        <RowConstraints minHeight="15"/>
        <RowConstraints minHeight="30"/>
        <RowConstraints minHeight="10"/>
        <RowConstraints vgrow="ALWAYS"/>
        <RowConstraints minHeight="15"/>
        <RowConstraints minHeight="50"/>
        <RowConstraints minHeight="15"/>
    </rowConstraints>
    <columnConstraints>
        <ColumnConstraints minWidth="15"/>
        <ColumnConstraints hgrow="ALWAYS"/>
        <ColumnConstraints minWidth="15"/>
    </columnConstraints>
    <Label styleClass="titlePane" text="Результат обучения" GridPane.columnIndex="1" GridPane.rowIndex="1"
           GridPane.halignment="CENTER"/>
    <Separator GridPane.columnIndex="1" GridPane.rowIndex="2"/>
    <TextArea GridPane.columnIndex="1" GridPane.rowIndex="3" editable="false" disable="true" focusTraversable="false"/>
    <ImageView fx:id="videoImageView" GridPane.columnIndex="1" GridPane.rowIndex="3"/>

    <TextArea fx:id="logsTextArea" GridPane.columnIndex="1" GridPane.rowIndex="5" editable="false" disable="true" focusTraversable="false"/>
</GridPane>
